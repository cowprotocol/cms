{
  "kind": "collectionType",
  "collectionName": "solver_bonding_pools",
  "info": {
    "singularName": "solver-bonding-pool",
    "pluralName": "solver-bonding-pools",
    "displayName": "Solver Bonding Pool",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "address": {
      "type": "string",
      "regex": "^(0x)?[0-9a-fA-F]{40}$",
      "required": true
    },
    "joinedOn": {
      "type": "datetime",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "solvers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::solver.solver",
      "inversedBy": "solver_bonding_pools"
    },
    "isReducedBondingPool": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "solver_networks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::solver-network.solver-network",
      "mappedBy": "vouchedBy"
    }
  }
}
